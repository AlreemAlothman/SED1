    _model.textController3 ??= TextEditingController(text: 'C  . .  t');
    _model.textFieldFocusNode3 ??= FocusNode();

    _model.textController4 ??= TextEditingController(text: 'G. . e . . n');
    _model.textFieldFocusNode4 ??= FocusNode();

